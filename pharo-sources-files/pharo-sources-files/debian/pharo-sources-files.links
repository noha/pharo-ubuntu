# /usr/share directory but the VM requires these files to be in the
# same directory as itself.
usr/share/pharo-vm/PharoV10.sources usr/lib/pharo-vm/PharoV10.sources
usr/share/pharo-vm/PharoV20.sources usr/lib/pharo-vm/PharoV20.sources
usr/share/pharo-vm/PharoV30.sources usr/lib/pharo-vm/PharoV30.sources
